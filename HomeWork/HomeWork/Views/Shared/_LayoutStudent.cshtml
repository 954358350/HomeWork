<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/css/core[1].css" rel="stylesheet" />
    <link href="~/css/style[1].css" rel="stylesheet" />
    <link href="~/css/font.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.7.1.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.20.min.js"></script>
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <title>@ViewBag.Title</title>
</head>
<body id="cc">
    <div>
        <div style="float: left">学员作业登记系统</div>
        <div style="float: right">你好小明，注销</div>
    </div>
    <br />
    <hr />
    <br />
    <div class="carousel slide" style="height:40px"><span style="float:left">&nbsp;&nbsp;&nbsp;&nbsp;</span>
        <div style="float:left;width:200px;">@Ajax.ActionLink("查询作业情况", "List", "Student", new AjaxOptions() { OnSuccess="a"})</div>
        <div style="float:left;width:200px;">@Ajax.ActionLink("上机练习", "Lianxi", "Student", new AjaxOptions() { OnSuccess="a"})</div>
        <div style="float:left;width:200px;">@Ajax.ActionLink("云题库", "Yuntiku", "Student", new AjaxOptions() { OnSuccess="a"})</div>
        <div style="float:left;width:auto;">@Ajax.ActionLink("预习总结", "Yuxi", "Student", new AjaxOptions() { OnSuccess="a"})</div>
        

    </div>
    <br />
    <hr />
    @RenderBody()
    <script>
        $.ajaxSetup({cache:false});
        function a(data) {
            $(".a").html(data);
        }
    </script>
</body>
</html>
